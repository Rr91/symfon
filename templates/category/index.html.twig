{% extends 'base.html.twig' %}

{% block title %}Hello CategoryController!{% endblock %}

{% block main %}
<h1>Category</h1>
    <ul>
        {% for category in categories %}
            <li><a href="{{ path('by_category', {category_id:category.id}) }}">{{ category.title }}</a></li>
        {% endfor %}
    </ul>
{% endblock %}
